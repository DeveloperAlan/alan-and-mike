<div class="jumbotron">
  <h1>Welcome</h1>
  <% if user_signed_in? && :search_list? %>
<p><%= link_to "Edit Search List", edit_search_list_path %></p>
<% elsif user_signed_in? && !:search_list? %>
<p><%= link_to "New Search List", new_search_list_path %></p>
<% else %>
<p>Welcome to Jigglr. Click on Sign up to sign up</p>
<% end %>
</div>