<%= form_for @searchlist do |f| %>

<ul>
<% @categories = Category.all %>
<% @categories.each do |category| %>
  <li>
    <%= check_box_tag category.id%> 
    <%= label_tag category.name %>
  </li>
<% end %>
</ul>

<div class="form-group">
  <%= f.submit class: "btn btn-lg btn-primary"  %>

<% end %>


