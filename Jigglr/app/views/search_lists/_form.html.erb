<% require "pry" %>

<%= form_for @searchlist do |f| %>

<div class="form_field">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<ul>
<% @categories = Category.all %>
<% @categories.each do |category| %>
  <li>
    <%= check_box_tag category.id%> 
    <%= label_tag category.name %>
  </li>
<% end %>
</ul>

<div class="form_field">
  <%= f.submit %>

<% end %>

